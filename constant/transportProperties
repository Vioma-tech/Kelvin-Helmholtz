FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases          (water d_water);

water
{
    transportModel  Newtonian;
    nu              1e-6;
    rho             1000;
}

d_water
{
    transportModel  Newtonian;
    nu              1e-6;
    rho             1030;
}

sigma            0.07;


